@misc{noauthor_acute_2020,
	title = {Acute kidney injury},
	copyright = {Creative Commons Attribution-ShareAlike License},
	url = {https://en.wikipedia.org/w/index.php?title=Acute_kidney_injury&oldid=950761333},
	abstract = {Acute kidney injury (AKI), previously called acute renal failure (ARF), is an abrupt loss of kidney function that develops within 7 days.Its causes are numerous. Generally it occurs because of damage to the kidney tissue caused by decreased kidney blood flow (kidney ischemia) from any cause (e.g., low blood pressure), exposure to substances harmful to the kidney, an inflammatory process in the kidney, or an obstruction of the urinary tract that impedes the flow of urine. AKI is diagnosed on the basis of characteristic laboratory findings, such as elevated blood urea nitrogen and creatinine, or inability of the kidneys to produce sufficient amounts of urine.
AKI may lead to a number of complications, including metabolic acidosis, high potassium levels, uremia, changes in body fluid balance, and effects on other organ systems, including death. People who have experienced AKI may have an increased risk of chronic kidney disease in the future. Management includes treatment of the underlying cause and supportive care, such as renal replacement therapy.},
	language = {en},
	urldate = {2020-05-20},
	journal = {Wikipedia},
	month = apr,
	year = {2020},
