@article{cha_collective_2023,
	title = {Collective {Resilience}: {Deterring} {China}'s {Weaponization} of {Economic} {Interdependence}},
	volume = {48},
	issn = {0162-2889},
	shorttitle = {Collective {Resilience}},
	url = {https://doi.org/10.1162/isec_a_00465},
	doi = {10.1162/isec_a_00465},
	abstract = {Since the 2010s, China has used economic coercion against Western and Asian
states to achieve territorial and political goals. China's leveraging of
its market is a form of ‚Äúpredatory liberalism‚Äù that weaponizes the
networks of interdependence created by globalization. The United States and
other like-minded partners have mostly used piecemeal ‚Äúde-risking‚Äù
measures such as decoupling, supply chain resilience, reshoring, and trade
diversion to reduce dependence on China and thereby minimize vulnerability to
its economic coercion. But these practices do not stop the Chinese
government's economic bullying. ‚ÄúCollective resilience‚Äù is
a peer competition strategy designed to deter the Xi Jinping regime's
economic predation. What informs this strategy is the understanding that
interdependence, even asymmetric interdependence, is a two-way street. Original
trade data show that the previous and current targets of economic coercion by
the Xi Jinping regime export over \$46.6 billion worth of goods to China
on which it is more than 70 percent dependent as a proportion of its total
imports of those goods. These target states could band together in a collective
resilience alliance and practice economic deterrence by promising to retaliate
against China's high-dependence trade should Beijing act against any one
of the alliance members.},
	number = {1},
	urldate = {2025-10-13},
	journal = {International Security},
	author = {Cha, Victor D.},
	month = jul,
	year = {2023},
	pages = {91--124},
	file = {Full Text PDF:C\:\\Users\\carpi\\Zotero\\storage\\7NYFYBPZ\\Cha - 2023 - Collective Resilience Deterring China's Weaponization of Economic Interdependence.pdf:application/pdf;Snapshot:C\:\\Users\\carpi\\Zotero\\storage\\WHAURYRW\\isec_a_00465.html:text/html},
}

