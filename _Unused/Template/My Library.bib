@article{anderson_measuring_2015,
	title = {Measuring breath acetone for monitoring fat loss: {Review}},
	volume = {23},
	issn = {1930-7381},
	shorttitle = {Measuring breath acetone for monitoring fat loss},
	url = {https://www.ncbi.nlm.nih.gov/pmc/articles/PMC4737348/},
	doi = {10.1002/oby.21242},
	abstract = {Objective
Endogenous acetone production is a by‚Äêproduct of the fat metabolism process. Because of its small size, acetone appears in exhaled breath. Historically, endogenous acetone has been measured in exhaled breath to monitor ketosis in healthy and diabetic subjects. Recently, breath acetone concentration (BrAce) has been shown to correlate with the rate of fat loss in healthy individuals. In this review, the measurement of breath acetone in healthy subjects is evaluated for its utility in predicting fat loss and its sensitivity to changes in physiologic parameters.

Results
BrAce can range from 1 ppm in healthy non‚Äêdieting subjects to 1,250 ppm in diabetic ketoacidosis. A strong correlation exists between increased BrAce and the rate of fat loss. Multiple metabolic and respiratory factors affect the measurement of BrAce. BrAce is most affected by changes in the following factors (in descending order): dietary macronutrient composition, caloric restriction, exercise, pulmonary factors, and other assorted factors that increase fat metabolism or inhibit acetone metabolism. Pulmonary factors affecting acetone exchange in the lung should be controlled to optimize the breath sample for measurement.

Conclusions
When biologic factors are controlled, BrAce measurement provides a non‚Äêinvasive tool for monitoring the rate of fat loss in healthy subjects.},
	number = {12},
	urldate = {2020-07-18},
	journal = {Obesity (Silver Spring, Md.)},
	author = {Anderson, Joseph C.},
	month = dec,
	year = {2015},
	pmid = {26524104},
