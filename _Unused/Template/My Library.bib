	author = {Yilmaz, Bahadƒ±r.},
	month = jun,
	year = {2013},
	note = {Conference Name: Sensors, and Command, Control, Communications, and Intelligence (C3I) Technologies for Homeland Security and Homeland Defense XII
ISBN: 9780819495020},
	pages = {87110W},
}

