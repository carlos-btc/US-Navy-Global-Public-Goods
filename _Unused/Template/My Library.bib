@techreport{bank_ethiopia_2020,
	title = {Ethiopia - {COVID}-19 {Emergency} {Response} {Project}},
	url = {http://documents.worldbank.org/curated/en/933841586193140802/Ethiopia-COVID-19-Emergency-Response-Project},
	abstract = {The objective of the COVID-19 (Coronavirus) Emergency Response Project is to prevent, detect and respond to the threat posed by COVID-19 (Coronavirus) and strengthen national systems for public health preparedness in Ethiopia. There are five components to the project, the first component being medical supplies and equipment. This component will finance the procurement of medical supplies and equipment needed for activities outlined in the EPRP such as (i) case management; (ii) infection prevention and control; and (iii) other pillars of the strategic plan. The second component is the preparedness, capacity building and training. This component will finance activities related to preparedness, capacity building and training, guided by the different pillars and activities of the EPRP. The third component is the community discussions and information outreach. Activities supported by this component include: risk communication and community engagement; behavioral and sociocultural risk factors assessments; production of RCCE strategy and training documents; production of communication materials; establish a production center for information and communication tools production center (media and community engagement); and monitoring and evidence generation; and human resources for risk communication. The fourth component is the quarantine, isolation and treatment centers establishment. Activities supported by this component include: establishing isolation and treatment centers, including provision of nutrition and dignity kits to quarantined patients, and equipping them with medical supplies and furniture and network installation. Finally, the fifth component is the project implementation and monitoring.},
	language = {en},
	number = {PAD3803},
	urldate = {2020-05-25},
	institution = {The World Bank},
	author = {Bank, The World},
	month = mar,
	year = {2020},
	pages = {1--0},
	file = {Snapshot:C\:\\Users\\carpi\\Zotero\\storage\\RUTZTSDW\\Ethiopia-COVID-19-Emergency-Response-Project.html:text/html},
}

