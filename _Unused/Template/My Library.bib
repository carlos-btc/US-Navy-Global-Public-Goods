@article{chezum_roses_1997,
	title = {Roses or {Lemons}: {Adverse} {Selection} in the {Market} for {Thoroughbred} {Yearlings}},
	volume = {79},
	issn = {0034-6535},
	shorttitle = {Roses or {Lemons}},
	url = {https://www.jstor.org/stable/2951401},
	abstract = {This paper tests for the presence of adverse selection in thoroughbred yearling auction markets. Thoroughbred auctions consist of two seller types: sellers who breed horses to race and sell (racers) and sellers who take all their yearlings to auction (breeders). If racers use private information, keeping those yearlings with a higher probability of on-track success, they are likely to receive a lower price for similar yearlings as compared to breeders. Using data from Keeneland's 1994 September yearling sale, we find support for this hypothesis. We improve on previous studies by analyzing the distinction between seller types on a continuous scale.},
	number = {3},
	urldate = {2020-09-23},
	journal = {The Review of Economics and Statistics},
	author = {Chezum, Brian and Wimmer, Brad},
	year = {1997},
	note = {Publisher: The MIT Press},
	pages = {521--526},
}

@article{wimmer_adverse_2006,
	title = {Adverse {Selection}, {Seller} {Effort}, and {Selection} {Bias}},
	volume = {73},
	issn = {0038-4038},
	url = {https://www.jstor.org/stable/20111883},
	doi = {10.2307/20111883},
	abstract = {Several studies (Genesove 1993; Chezum and Wimmer 1997) use evidence of a correlation between seller characteristics and prices as evidence of adverse selection. This approach ignores the effect seller effort might have on the quality of goods sold. We develop a theoretical framework that accounts for both adverse selection and seller effort and provide a set of conditions under which sellers, who more likely adversely select the goods they sell, produce higher quality goods. Empirically, adverse selection emerges as a special case of selection bias. To disentangle the effects of adverse selection from seller effort, we employ a unique data set that allows us to model the selection decision explicitly. The evidence suggests that both adverse selection and hidden effort play important roles in the market for thoroughbred racehorse prospects.},
	number = {1},
	urldate = {2020-09-23},
	journal = {Southern Economic Journal},
	author = {Wimmer, Bradley S. and Chezum, Brian},
	year = {2006},
	note = {Publisher: Southern Economic Association},
	pages = {201--218},
}

