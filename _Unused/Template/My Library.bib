@article{leeper_search_1992,
	title = {In search of the liquidity effect},
	volume = {29},
	issn = {0304-3932},
	url = {http://www.sciencedirect.com/science/article/pii/030439329290031V},
	doi = {10.1016/0304-3932(92)90031-V},
	abstract = {A short-run negative relationship between monetary aggregates and interest rates ‚Äî the ‚Äòliquidity effect‚Äô ‚Äî is central to discussions of monetary policy. This paper searches for this empirical relationship. We investigate whether the characterization of the liquidity effect is sensitive to: (i) changes in sample period, (ii) conditioning the correlations on past information, (iii) assuming money growth is exogenous, and (iv) treating monetary changes as anticipated or unanticipated. The correlations change substantially in each case. We conclude that the traditional analysis and modern models, which rely completely on demand-side behavior, cannot explain the observed correlations. A successful characterization of the liquidity effect requires identification of both private and policy behavior.},
	language = {en},
	number = {3},
	urldate = {2020-06-11},
